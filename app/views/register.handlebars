<div class="row">
    <form class="col s12 m6 offset-m3 center" id="myform">
        <div class="section center">
            <h4>Register Here</h4>
        </div>
        <div class="col m6">
            <label for="first_name">First Name</label>
            <input id="first_name">
        </div>
        <div class="col m6">
            <label for="last_name">Last Name</label>
            <input id="last_name">
        </div>
        <div class="col m12">
            <label for="user_name">Username </label>
            <input id="user" name="user">
        </div>
        <div class="col m6">
            <label for="password">Password </label>
            <input id="password" name="password">
        </div>
        <div class="col m6">
            <label for="password_check">Re-Enter Password</label>
            <input id="password_check" name="password_check">
        </div>
        <br>
        <input class="waves-effect waves-light btn submit-btn" type="submit" value="Validate!">
    </form>
</div>
