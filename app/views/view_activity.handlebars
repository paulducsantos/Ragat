<div class="container">
    <div class="row">
        <div class="col m6 s12 offset-m3">
            <div class="title">
                {{#each data}}
                <h4>{{ name }}</h4> {{/each}}
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col m6 s12 offset-m3">
            <div class="reviews">
                <form method="POST" action="/addReview">
                    <p class="range-field">
                        <input type="range" id="test5" min="0" max="5" />
                    </p>
                    <textarea id="textarea1" class="materialize-textarea" length="500" name="review"></textarea>
                    <button type="submit" class="btn-floating btn-large waves-effect waves-light red">submit</button>
                </form>
                <ul>
                    {{#each reviewPost}}
                    <li>{{ review }}</li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>